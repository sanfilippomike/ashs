<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-10">
        <h2>Contact Us</h2>
        <ol class="breadcrumb">
    </ol>
</div>
</div>

<div class="wrapper wrapper-content animated fadeInRight">
<div class="row">
    <div class="col-md-6">
        <div class="contact-box">
            <div class="col-md-4">
                <div class="text-center">
                    <img alt="image" class="img-circle m-t-xs img-responsive" src="<%= image_path('contact.jpg') %>">
                    <div class="m-t-xs font-bold">Student Health Services</div>
                </div>
            </div>
            <div class="col-md-8">
                <h3><strong>H. Rajender Reddy Health Center</strong></h3>
                <address>
                    <strong>University of California, Merced </strong><br>
                    5200 North Lake Road <br>
                    Merced, CA 95343<br>
                    <abbr title="Phone">Phone:</abbr> (209) 228-2273
                    <br>
                    <abbr title="Fax">Fax:</abbr> (209) 228-7650
                    <br>
                    <a href="mailto:health@ucmerced.edu?subject=Please do not e-mail any private health information">health@ucmerced.edu</a>
                    <p class="label label-danger"> Never E-mail PHI  </p>
                </address>
            </div>
            <div class="clearfix"></div>
    </div>
</div>


<div class="row">
    <div class="col-md-6">
        <a href="https://myhealth.ucmerced.edu" target="_blank">
        <div class="contact-box">
            <div class="col-md-4">
                <div class="text-center">
                    <div class ="darkblue">
                        <i class="fa fa-h-square fa-5x"></i>
                        <div class="m-t-xs font-bold"><i class="fa fa-h-square"></i> myHealth-Messaging</div>
                    </div>
                </div>
            </div>
            <div class="col-md-8">
                <h3><strong>To send a secure message</strong></h3>
                <address>
                    <strong>log into <i class="fa fa-h-square"></i> myHealth-Messaging</strong><br>
                    <a href="https://myhealth.ucmerced.edu" target="_blank">https://myhealth.ucmerced.edu</a>
                </address>
            </div>
            <div class="clearfix"></div>
         </a>
    </div>
</div>
</div>


<div class="row">
    <div class="col-md-12">
        <img alt="image" class="img-responsive" src="<%= image_path('campusmap.jpg') %>">
    </div>
</div>


</div>
</div>

